{
  "name": "teacher-schedule",
  "version": "2.5.4",
  "description": "Современное веб-приложение для отображения расписания преподавателей с поддержкой выходных дней, темной темы и умной фильтрацией",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server -p 8000 -o",
    "dev": "npx live-server --port=8000 --open=/index.html",
    "serve": "python -m http.server 8000"
  },
  "keywords": [
    "schedule",
    "teachers",
    "university",
    "timetable",
    "holidays",
    "dark-theme",
    "responsive"
  ],
  "author": "Schedule App",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/teacher-schedule.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/teacher-schedule/issues"
  },
  "homepage": "https://github.com/your-username/teacher-schedule#readme"
}
